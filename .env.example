# Learning Kingdom environment template
# Copy to .env or .env.local for local development
# For Vite, all public env vars must start with VITE_

VITE_FIREBASE_API_KEY=changeme
VITE_FIREBASE_AUTH_DOMAIN=changeme.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=changeme
VITE_FIREBASE_STORAGE_BUCKET=changeme.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=000000000000
VITE_FIREBASE_APP_ID=1:000000000000:web:changeme
VITE_FIREBASE_DATABASE_URL=https://changeme-default-rtdb.firebaseio.com

# Optional: Analytics
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Optional: App Check (for production)
VITE_FIREBASE_APPCHECK_SITE_KEY=your-recaptcha-v3-site-key
VITE_FIREBASE_APPCHECK_DEBUG_TOKEN=your-debug-token-for-local-dev
